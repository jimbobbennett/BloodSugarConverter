<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.jimbobbennett.BloodSugarConverter" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Blood Sugar Converter</name>
  <description>
    A simple Cordova based project that converts bloog sugar measurements from US to International units and vice versa.
  </description>
  <author href="http://jimbobbennett.io" email="jimbobbennett@outlook.com">
    Jim Bennett
  </author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="Orientation" value="portrait" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="Fullscreen" value="true" />
  <vs:features />
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows8" />
  </vs:platformSpecificValues>
</widget>
